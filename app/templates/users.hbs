<div class="jumbo">
    <div class="right tomster"></div>
    <h2>Users list</h2>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            {{#each model as |user|}}
            <tr>
                <td>{{user.user_id}}</td>
                <td>{{user.user_first_name}} {{user.user_last_name}}</td>
                <td></td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>